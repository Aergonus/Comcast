#include <string>
#include "node.h"
packet(std::string id, node* src, node* dst, flow* fl){
  id_ = id;
  origin_ = src;
  dest_ = dst;
  flow_ = fl;
}

node* origin(){
  return origin_;
}
node* dest(){
  return dest_;
}
int size(){
  return size_;
}
int seqNum(){
  return seqNum_;
}
std::string data(){
  return data_;
}
